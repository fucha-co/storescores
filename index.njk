---
maxResults: 10
layout: layout.njk
pagination:
  data: "sites"
  size: 1
  alias: vertical
permalink: "/"
counts:
  performance: 0
  accessibility: 0
  bestPractices: 0
  seo: 0
  perfect: 0
  total: 0
---
		
<div class="">
					


{%- set showRank = true %}
    {%- for site in results | getSites(sites, vertical, isolatedKeys) | sortCumulativeScore %}
        {%- set newestKey = site | getObjectKey(":newest") %}
        {%- set key = site | getObjectKey %}
        {%- set hundos = 0 %}

        {%- if not site[newestKey].error %}
            {%- set hundos = site[newestKey] | hundoCount %}
            {%- set notGreenCircleCount = site[newestKey] | notGreenCircleCount %}
            {%- if notGreenCircleCount >= 3 and sites[vertical].options and sites[vertical].options.noShame %}{% set showRank = false %}{% endif %}
        {% endif %}
			
	<!-- Six columns -->
	<div class="relative max-w-screen-2xl my-6 mx-auto px-2 sm:px-2 lg:px-2">
		<div class="max-w-6xl mx-auto">
		
			<div x-data="{ open: false }" class="px-3 py-0 rounded-lg text-black bg-white net-shadow">
				<!-- Six columns -->

				<div @click="open = true" class="flex flex-row">

					<div class="flex flex-row w-1/3 leading-tight py-6">
						<div class="pr-3 text-2xl">
							{% if hundos === 4 %}üèÜ{% endif %}
							{% if loop.index0 === 0 %}ü•á
							{% elseif loop.index0 === 1 %}ü•à
							{% elseif loop.index0 === 2 %}ü•â
							{% elseif showRank %}
								<span class="px-0 -mt-4">
									<span class="inline-flex items-center px-2 py-1 rounded-full text-sm font-medium leading-4 bg-gray-200 text-gray-800">
									{{ loop.index }}
								</span>
								</span>
							{% endif %}
						</div>
						<div class="flex flex-col">
							<div class="font-semibold text-red-600 text-2xl url">
								{{ site[newestKey].url | displayUrl(hasRedirect) | replace("/", "") }}
								<span class="block text-base font-normal text-gray-500">
								
							{% if site[key].url == "https://www.pushas.com/" %}Shopify Website{%- endif %}
							{% if site[key].url === "https://ivyandwood.com.au/" %}Shopify Website{%- endif %}
							{% if site[key].url === "https://sensorylab.com.au/" %}Shopify Website{%- endif %}
							{% if site[key].url == "https://stali.com.au/" %}Shopify Plus Website{%- endif %}
							{% if site[key].url === "https://au.koala.com/" %}Shopify Plus Website{%- endif %}
							{% if site[key].url === "https://shopmrbeast.com/" %}Shopify Plus Website{%- endif %}
							{% if site[key].url === "https://www.culturekings.com.au/" %}Shopify Plus Website{%- endif %}
							{% if site[key].url == "https://teamapparel.com.au/" %}WordPress Woocommerce Website{%- endif %}
							{% if site[key].url == "https://debut-theme-lightroom.myshopify.com/" %}Shopify theme{%- endif %}
							{% if site[key].url == "https://debut-theme-default.myshopify.com/" %}Shopify theme{%- endif %}
							</span>
						</div>
							
						</div>
					</div>
					
					<div class="px-6 py-6 text-center leading-tight bg-gray-100">
						<div class="text-2xl font-semibold txt-blk">{% if hundos === 4 %}üíØ{% else %}{{ site[newestKey] | lighthouseTotal//4 | round | int }}%{% endif %}</div>
						<div class="flex-row text-sm text-gray-400 font-medium uppercase">Score</div>
					</div>

					{%- if not site[newestKey].error %}
					
					<div class="flex-1 text-center pt-6 pb-5 leading-tight">
						<div class="flex-row">
							<span class="text-2xl font-semibold txt-blk">
								{{ (site[newestKey].lighthouse.performance * 100) | round }}
							</span>
							<span class="text-2xl font-medium text-gray-400 -ml-1">/100</span>
						</div>
						<span class="text-sm font-semibold text-red-600 uppercase block">Speed</span>
					</div>
					<div class="flex-1 text-center pt-6 pb-5 leading-tight">
						<div class="flex-row">
							<span class="text-2xl font-semibold txt-blk">
								{{ (site[newestKey].lighthouse.seo * 100) | round }}
							</span>
							<span class="text-2xl font-medium text-gray-400 -ml-1">/100</span>
						</div>
						<span class="text-sm font-semibold text-red-600 uppercase block">SEO</span>
					</div>
					<div class="flex-1 text-center pt-6 pb-5 leading-tight">
						<div class="flex-row">
							<span class="text-2xl font-semibold txt-blk">
								{{ (site[newestKey].lighthouse.accessibility * 100) | round }}
								</span>
							<span class="text-2xl font-medium text-gray-400 -ml-1">/100</span>
						</div>
						<span class="flex-row text-sm font-semibold text-red-600 uppercase block">Access</span>
					</div>
					<div class="flex-1 text-center pt-6 pb-5 leading-tight">
						<div class="flex-row">
							<span class="text-2xl font-semibold txt-blk">
								{{ (site[newestKey].lighthouse.bestPractices * 100) | round }}
							</span>
							<span class="text-2xl font-medium text-gray-400 -ml-1">/100</span>
						</div>
						<span class="flex-row text-sm font-semibold text-red-600 uppercase block">Quality</span>
					</div>
					<div class="pt-6 pb-5 pl-3 pr-5 text-center leading-tight">
						<span class="flex-row text-2xl font-medium text-gray-400 -ml-1">></span>
					</div>
						
					{%- endif %}

					<div x-show="open" @click.away="open = false">
					Last tested {{ site[newestKey].timestamp | displayDate | safe }}!
					</div>
				</div>
			</div>

		</div>
	</div>

	{%- endfor %}

</div>